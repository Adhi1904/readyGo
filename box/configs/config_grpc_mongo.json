{
    "version": "0.1",
    "kind": "grpc",
    "project": "grpc_person_sample",
    "databaseSpec": {
      "kind": "mongo",
      "connectionString": "mongodb://localhost:27017",
      "name": "sample"
    },
    "apiSpec": {
      "kind": "grpc",
      "port": "50057",
      "version": "v1"
    },
    "models": [
      {
        "name": "person",
        "fields": [
          {
            "name": "name",
            "type": "string",
            "isKey": true
          },
          {
            "name": "email",
            "type": "string",
            "validateExp": "[a-zA-Z0-9]",
            "isKey": true
          },
          {
            "name": "mobile",
            "type": "string"
          },
          {
            "name": "status",
            "type": "string"
          },
          {
            "name": "lastModified",
            "type": "string"
          }
        ]
      },
      {
        "name": "address",
        "fields": [
          {
            "name": "line",
            "type": "string"
          },
          {
            "name": "street",
            "type": "string",
             "isKey": false
          },
          {
            "name": "city",
            "type": "string"
          },
          {
            "name": "state",
            "type": "string"
          },
          {
            "name": "country",
            "type": "string"
          },
          {
            "name": "zip",
            "type": "string"
          },
          {
            "name": "status",
            "type": "string"
          },
          {
            "name": "lastModified",
            "type": "string"
          }
        ]
      }
    ]
  }