{
  "version": "0.1",
  "kind": "http",
  "project": "contacts",
  "databaseSpec": {
    "kind": "mongo",
    "connectionString": "mongodb://localhost:27017",
    "name": "contacts"
  },
  "apiSpec": {
    "kind": "http",
    "port": "50058",
    "version": "v1"
  },
  "models": [
    {
      "name": "person",
      "fields": [
        {
          "name": "name",
          "type": "string",
          "isKey": true
        },
        {
          "name": "description",
          "type": "string",
          "validateExp": "[a-zA-Z0-9]"
        },
        {
          "name": "refNo",
          "type": "long"
        },
        {
          "name": "status",
          "type": "string"
        },
        {
          "name": "lastModified",
          "type": "string"
        },
        {
          "name": "Address",
          "type": "Address"
        }
      ]
    },
    {
      "name": "Address",
      "fields": [
        {
          "name": "houseno",
          "type": "string"
        },
        {
          "name": "street",
          "type": "string"
        },
        {
          "name": "state",
          "type": "string"
        },
        {
          "name": "country",
          "type": "string"
        },
        {
          "name": "zip",
          "type": "string"
        }
      ]
    }
  ]
}